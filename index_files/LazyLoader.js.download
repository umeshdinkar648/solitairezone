const LazyLoader={lazyLoadCssBackground:function(jQueryElement,backgroundImageUrl,vignetteDefinition,placeholderColor){jQueryElement.css('background-image','none');if(!!placeholderColor){jQueryElement.css('background-color',placeholderColor)}
$('<img/>').attr('src',backgroundImageUrl).on('load',function(){$(this).remove();jQueryElement.css('background-color','inherit');if(!!vignetteDefinition){jQueryElement.css('background-image',vignetteDefinition+", url('"+backgroundImageUrl+"')")}else{jQueryElement.css('background-image',"url('"+backgroundImageUrl+"')")}
console.log("Loaded css def.: "+vignetteDefinition+", url('"+backgroundImageUrl+"')")})}}